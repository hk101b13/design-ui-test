#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/weizhaoma/DesignSystem/design-ui-test/design-ui-test/node_modules/.pnpm/@microsoft+api-extractor@7.39.0_@types+node@20.10.5/node_modules/@microsoft/api-extractor/bin/node_modules:/home/weizhaoma/DesignSystem/design-ui-test/design-ui-test/node_modules/.pnpm/@microsoft+api-extractor@7.39.0_@types+node@20.10.5/node_modules/@microsoft/api-extractor/node_modules:/home/weizhaoma/DesignSystem/design-ui-test/design-ui-test/node_modules/.pnpm/@microsoft+api-extractor@7.39.0_@types+node@20.10.5/node_modules/@microsoft/node_modules:/home/weizhaoma/DesignSystem/design-ui-test/design-ui-test/node_modules/.pnpm/@microsoft+api-extractor@7.39.0_@types+node@20.10.5/node_modules:/home/weizhaoma/DesignSystem/design-ui-test/design-ui-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/weizhaoma/DesignSystem/design-ui-test/design-ui-test/node_modules/.pnpm/@microsoft+api-extractor@7.39.0_@types+node@20.10.5/node_modules/@microsoft/api-extractor/bin/node_modules:/home/weizhaoma/DesignSystem/design-ui-test/design-ui-test/node_modules/.pnpm/@microsoft+api-extractor@7.39.0_@types+node@20.10.5/node_modules/@microsoft/api-extractor/node_modules:/home/weizhaoma/DesignSystem/design-ui-test/design-ui-test/node_modules/.pnpm/@microsoft+api-extractor@7.39.0_@types+node@20.10.5/node_modules/@microsoft/node_modules:/home/weizhaoma/DesignSystem/design-ui-test/design-ui-test/node_modules/.pnpm/@microsoft+api-extractor@7.39.0_@types+node@20.10.5/node_modules:/home/weizhaoma/DesignSystem/design-ui-test/design-ui-test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/api-extractor" "$@"
else
  exec node  "$basedir/../../bin/api-extractor" "$@"
fi
